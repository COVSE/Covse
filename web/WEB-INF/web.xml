<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Autenticacion</servlet-name>
        <servlet-class>Controlador.Autenticacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaestroUsuarios</servlet-name>
        <servlet-class>Controlador.MaestroUsuarios</servlet-class>
    </servlet>
   
     <servlet>
        <servlet-name>MaestroClientes</servlet-name>
        <servlet-class>Controlador.MaestroClientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaestroPedidos</servlet-name>
        <servlet-class>Controlador.MaestroPedidos</servlet-class>
    </servlet>
       <servlet>
        <servlet-name>MaestroProveedores</servlet-name>
        <servlet-class>Controlador.MaestroProveedores</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MaestroProduc</servlet-name>
        <servlet-class>Controlador.MaestroProduc</servlet-class>
    </servlet>
     
    <servlet-mapping>
        <servlet-name>Autenticacion</servlet-name>
        <url-pattern>/Autenticacion</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>MaestroUsuarios</servlet-name>
        <url-pattern>/MaestroUsuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaestroPedidos</servlet-name>
        <url-pattern>/MaestroPedidos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaestroClientes</servlet-name>
        <url-pattern>/MaestroClientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MaestroProveedores</servlet-name>
        <url-pattern>/MaestroProveedores</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>MaestroProduc</servlet-name>
        <url-pattern>/MaestroProduc</url-pattern>
    </servlet-mapping>
    
   
   
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
